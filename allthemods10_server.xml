<?xml version="1.0" encoding="utf-8"?>

<Container version="2">
  <Name>All The Mods 10</Name>
  <Repository>r14f/allthemods10</Repository>
  <Registry>https://hub.docker.com/repository/docker/r14f/allthemods10</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>TO BE ADDED STILL</Support>
  <Project>https://www.curseforge.com/minecraft/modpacks/all-the-mods-10</Project>
  <Overview>[b]All the Mods 10-4.0[/b][br]Shaders, Quests, Neoforge: 21.1.8 and tons of mods/fun.[br]Version tags available:[br] - v4.0[br] [br][br][br]Setup:[br][li]You must accept the EULA by setting the [b]EULA[/b] variable to [span style='color: #00E800;']true[/span] or the install will fail.[br]Optional:[br][li]You can setup a comma separated list of players who will have admin privileges in game by setting [b]OPS[/b]</Overview>
  <Category>GameServers:</Category>
  <ExtraSearchTerms>minecraft</ExtraSearchTerms>
  <Icon>https://media.forgecdn.net/avatars/thumbnails/1182/438/64/64/638755918649288941.png</Icon>
<Config 
    Name="Installed Location" 
    Target="/data" Default="/mnt/user/appdata/allthemods10/" 
    Mode="rw" Description="Path to store the installed server files." 
    Type="Path" 
    Display="always" 
    Required="true" 
    Mask="false">/mnt/user/appdata/allthemods10/</Config>
